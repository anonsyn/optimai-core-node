{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/main/**/*.d.ts",
    "src/main/**/types.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "noImplicitReturns": false,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/renderer/src/assets/*"
      ],
      "@lite-node/*": [
        "src/renderer/src/apps/lite-node/*"
      ],
      "@core-node/*": [
        "src/renderer/src/apps/core-node/*"
      ],
      "@main/*": [
        "src/main/*"
      ],
      "@/*": [
        "src/renderer/src/*",
      ],
    }
  }
}